{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "eslint": "node_modules/.bin/eslint src/**",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx nodemon server.js",
    "migrate": "npx knex migrate:latest --knexfile db/knexfile.js",
    "seed": "npx knex seed:run --knexfile db/knexfile.js",
    "seedTestDocuments": "npx knex seed:run --specific=seedTestDocuments.js --knexfile db/knexfile.js",
    "seedTestRoutes": "npx knex seed:run --specific=seedTestRoutes.js --knexfile db/knexfile.js",
    "seedTestForms": "npx knex seed:run --specific=seedTestForms.js --knexfile db/knexfile.js",
    "down": "npx knex migrate:down --knexfile db/knexfile.js",
    "rollback": "npx knex migrate:rollback --all --knexfile db/knexfile.js",
    "precommit": "lint-staged",
    "prettier": "prettier --write src/**/*.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.1.0",
    "body-parser": "1.20.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "express-validator": "6.14.2",
    "formidable": "^2.1.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "8.5.1",
    "knex": "2.4.2",
    "libreoffice-convert": "1.4.1",
    "moment": "2.29.4",
    "multer": "1.4.3",
    "nodemailer": "6.8.0",
    "nodemon": "2.0.20",
    "passport": "0.6.0",
    "pg": "8.8.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "node_modules/.bin/eslint --max-warnings=0",
      "prettier --write",
      "git add"
    ]
  }
}
