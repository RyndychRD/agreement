{
  "name": "agreementclient",
  "version": "1.0.0",
  "author": "ZavodKirow",
  "description": "agreement client",
  "main": "public/electron.js",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.8.0",
    "@ant-design/pro-components": "2.3.46",
    "@ant-design/pro-layout": "7.3.9",
    "@reduxjs/toolkit": "1.9.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/axios": "0.14.0",
    "antd": "5.0.4",
    "antd-table-saveas-excel": "^2.2.1",
    "axios": "1.2.1",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "qrcode.react": "^3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.4.5",
    "react-scripts": "5.0.1",
    "react-to-print": "2.14.11",
    "react-use-websocket": "^4.3.1",
    "redux-thunk": "2.4.2",
    "serve": "^14.2.0",
    "video-react": "^0.16.0",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "eslint": "node_modules/.bin/eslint --quiet src/**/*.jsx",
    "prettier": "prettier --write src/**/*.jsx",
    "start": "set REACT_APP_NODE_ENV=development && set HTTPS=true&& react-scripts start",
    "buildTest": "set REACT_APP_NODE_ENV=testing && set HTTPS=true&& react-scripts build",
    "buildProd": "set REACT_APP_NODE_ENV=production && set HTTPS=true&& react-scripts build",
    "prodStart": "serve -s build",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "docs:build": "documentation build src/** -f html -o docs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "node_modules/.bin/eslint --max-warnings=0",
      "prettier --write",
      "git add"
    ]
  }
}
